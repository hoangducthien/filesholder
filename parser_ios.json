[
  {
    "domain": "ntruyen.top",
    "keys": [
      "gicungco",
      "ntruyen"
    ],
    "title": {
      "selector": "#story_name",
      "value": "value"
    },
    "chapter": {
      "selector": ".chapter-infos h1"
    },
    "content": {
      "selector": "#chapter-content"
    },
    "next_button": {
      "selector": ".chapter-direction a",
      "element_indexed": 2,
      "value": "href"
    },
    "pre_button": {
      "selector": ".chapter-direction a",
      "value": "href"
    },
    "use_webview": false,
    "use_html_parser": true,
    "chapter_ignore_domain": true
  }
]
