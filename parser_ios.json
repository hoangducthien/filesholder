      "selector": ".chapter-text-info"
    },
    "content": {
      "selector": "#chapter-content"
    },
    "next_button": {
      "selector": "#next_chap",
      "value": "href"
    },
    "pre_button": {
      "selector": "#prev_chap",
      "value": "href"
    }
  },
  {
    "domain": "ntruyen.top",
    "keys": [
      "gicungco",
      "ntruyen"
    ],
    "title": {
      "selector": "#story_name",
      "value": "value"
    },
    "chapter": {
      "selector": ".chapter-infos h1"
    },
    "content": {
      "selector": "#chapter-content"
    },
    "next_button": {
      "selector": ".chapter-direction a",
